using Gtk 4.0;
using Adw 1;

template $FileInstallDialog : Adw.AlertDialog {
    follows-content-size: true;
    Box {
        orientation: vertical;
        spacing: 12;
        Adw.PreferencesGroup packages_group {
        }
        $InstallationChooser installation_chooser {
        }
    }
    responses [
        cancel: _("Cancel"),
        continue: _("Add") suggested,
    ]
}
